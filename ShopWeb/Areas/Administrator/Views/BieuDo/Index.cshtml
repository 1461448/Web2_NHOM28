@using WebShop.Areas.Administrator.Models
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}

<div >
    <div class="page-title">
        <i class="fa fa-bar-chart-o"></i>
        <h3>@ViewBag.titles</h3>
        <div class="pull-right actions">
            <button class="btn btn-primary btn-cons" type="button" id="btn-new-ticket">@Html.ActionLink("Thêm mới", "ThemMoi")</button>
        </div>
    </div>
    <div class="clearfix"></div>
</div>
<div style="background-color:white;">
    <center>
        <table>
            <tr>
                <td>
                    <div style="background-color:white;" class="horizontal">
                        <center>
                            
                            <img src="@Url.Content("~/Images/charts/" + ViewBag.imagePath)" />
                        </center>
                    </div>
                </td>
            </tr>

        </table>
    </center>
</div>
